{
	"name": "react-spinners-workspace",
	"version": "0.1.2",
	"private": true,
	"workspaces": [
		"packages/*"
	],
	"overrides": {
		"react": "^19.2.0",
		"react-dom": "^19.2.0"
	},
	"scripts": {
		"build": "npm run build -w packages/react-spinners",
		"build:demo": "npm run build:demo -w packages/demo",
		"dev": "npm run dev -w packages/demo",
		"release": "npm run build && npm publish -w packages/react-spinners"
	}
}
